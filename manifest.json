{
  "manifest_version": 2,
  "name": "Fruit Watch Ultra",
  "version": "0.2",
  "applications": {
    "gecko": {
      "strict_min_version": "57.0"
    }
  },
  "icons": {
    "128": "fwu.png"
  },
  "permissions": [
    "alarms",
    "theme"
  ],
  "background": {
    "scripts": [
      "fwu.js"
    ]
  },
  "default_locale": "en"
}